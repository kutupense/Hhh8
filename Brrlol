local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()

local Window = Fluent:CreateWindow({
    Title = "üåå Galaxy Hub",
    SubTitle = "Broken Bones IV",
    TabWidth = 130,
    Size = UDim2.fromOffset(480, 360),
    Theme = "Amethyst",
    MinimizeKey = Enum.KeyCode.LeftControl
})

local StatsTab = Window:AddTab({ Title = "Stats", Icon = "book" })

StatsTab:AddParagraph({
    Title = "Details ‚ÑπÔ∏è",
    Content = "When you use a button,it will not work or show in the game \nRejoin to see results."
})

local StatsSection = StatsTab:AddSection("Stats Section")

StatsSection:AddButton({
    Title = "+100b Money üí∏",
    Description = "Set Stats:Money to 100b",
    Callback = function()
        local args = {
            [1] = "money",
            [2] = 1000000000
        }
        game:GetService("ReplicatedStorage").Functions.UpdateStat:InvokeServer(unpack(args))
    end
})

StatsSection:AddButton({
    Title = "+100 Break Level ü¶¥",
    Callback = function()
        local args = {
            [1] = "breakslevel",
            [2] = 100
        }
        game:GetService("ReplicatedStorage").Functions.UpdateStat:InvokeServer(unpack(args))
    end
})

StatsSection:AddButton({
    Title = "+100 Sprains Level üíî",
    Callback = function()
        local args = {
            [1] = "sprainslevel",
            [2] = 100
        }
        game:GetService("ReplicatedStorage").Functions.UpdateStat:InvokeServer(unpack(args))
    end
})

StatsSection:AddButton({
    Title = "+100 Dislocations Level üìª",
    Callback = function()
        local args = {
            [1] = "dislocationslevel",
            [2] = 100
        }
        game:GetService("ReplicatedStorage").Functions.UpdateStat:InvokeServer(unpack(args))
    end
